import java.util.*;
import java.io.*;
/**
 * @author Alexander Caruso
 *
 */
public class BasicFileReader {

	/**
	 * @param args
	 * @throws FileNotFoundException 
	 */
	public static void main(String[] args) throws FileNotFoundException {
		
		Scanner input = new Scanner(new File ("src/input.txt"));
		PrintStream output = new PrintStream(new File ("src/output.txt"));
		
		while (input.hasNextLine()) {
			totalCounter(input.nextLine(), output);
		}
		
		input.close();
		output.close();
		
	}
	
	public static void totalCounter (String line, PrintStream output) {
		
		Scanner inputReader = new Scanner(line);
		int accountNumber = 0;
		
		inputReader.next();
		
		inputReader.next();
		if (inputReader.next() == ("Checking")) {
			accountNumber = 1;
		} else if (inputReader.next() == ("Savings")) {
			accountNumber = 2;
		} else {
			accountNumber = 3;
		}
		
		

		inputReader.close();
		
	}

}

import java.util.*;
import java.io.*;
/**
 * @author acaruso
 *
 */
public class ArrayTrainer {

	/**
	 * @param args
	 * @throws FileNotFoundException 
	 */
	public static void main(String[] args) throws FileNotFoundException {
		
		//Make the sort of data structure I'll be practicing.
		ArrayList <String> test = new ArrayList();
		System.out.println("Printing ArrayList items.");
		
		//Add stuff to the test data structure.
		test.add("A");
		test.add("B");
		test.add("C");
		test.add("C");
		test.add("D");
		test.add("D");
		
		//Try removing several elements.
		test.remove("C");
		test.remove("C");
		
		//Print.
		System.out.println("Foreach:");
		for (String s : test) {
			System.out.println(s);
		}
		System.out.println("Iterator:");
		Iterator <String> i = test.iterator();
		while (i.hasNext()) {
			System.out.println(i.next());
		}
		
	}
}
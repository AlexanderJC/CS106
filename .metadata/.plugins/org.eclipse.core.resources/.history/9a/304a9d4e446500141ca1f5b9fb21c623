import java.util.*;
public class Competition {

	public static void main(String[] args) {
		
		int dataNum = 0;
		
		Scanner input = new Scanner(System.in);
		System.out.println("Number of data sets?");
		dataNum = input.nextInt();
		
		int[] questions = new int[dataNum * 2];
		
		System.out.println("First problem?");
		questions[0] = input.nextInt();
		questions[1] = input.nextInt();
		
		System.out.println("Second problem?");
		questions[2] = input.nextInt();
		questions[3] = input.nextInt();
		
		System.out.println("Third problem?");
		questions[4] = input.nextInt();
		questions[5] = input.nextInt();
		
		System.out.println("Fourth problem?");
		questions[6] = input.nextInt();
		questions[7] = input.nextInt();
		
		System.out.println("Fifth problem?");
		questions[8] = input.nextInt();
		questions[9] = input.nextInt();
		
		System.out.println("You get " + (questions[0] / questions[1]) + " piece(s) and your dad gets " + (questions[0] % questions[1]) + " piece(s).");
		
	}

}
